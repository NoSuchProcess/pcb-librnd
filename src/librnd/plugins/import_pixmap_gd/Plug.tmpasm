put /local/pcb/mod {import_pixmap_gd}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/import_pixmap_gd/import_pixmap_gd.o @]

switch /local/pcb/import_pixmap_gd/controls
	case {disable} end;
	default
		put /local/pcb/mod/LDFLAGS         libs/gui/gd/ldflags
		put /local/pcb/mod/CFLAGS          libs/gui/gd/cflags
		end
end

switch /local/pcb/import_pixmap_gd/controls
	case {buildin}   include /local/librnd/tmpasm/buildin; end;
	case {plugin}    include /local/librnd/tmpasm/plugin; end;
	case {disable}   include /local/librnd/tmpasm/disable; end;
end
